{
    "swagger": "2.0",
    "info": {
        "title": "SyncStage Public Developer API",
        "version": "1.0.0"
    },
    "paths": {
        "/developer-api/login": {
            "post": {
                "tags": [
                    "Authorization"
                ],
                "description": "Method for singin in to developer API, returning jwt must be used in bearer Authorization in other methods",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "Body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "required": [
                                "token"
                            ],
                            "properties": {
                                "token": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "uniqueItems": false,
                            "type": "object",
                            "properties": {
                                "jwt": {
                                    "type": "string"
                                },
                                "authorizationType": {
                                    "type": "string"
                                },
                                "expiresIn": {
                                    "type": "integer"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": []
            }
        },
        "/developer/recordings": {
            "get": {
                "summary": "getSessionRecordings",
                "description": "Get Recordings for a session (Developer API)",
                "tags": [
                    "Recordings"
                ],
                "operationId": "getSessionRecordings.get./developer/recordings",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "sessionCode",
                        "type": "string",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of recordings",
                        "schema": {
                            "$ref": "#/definitions/IRecordingsResponse"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden",
                        "schema": {
                            "$ref": "#/definitions/IErrorResponse"
                        }
                    }
                }
            }
        },
        "/developer/usage": {
            "get": {
                "summary": "getUsage",
                "description": "Gets usage data of organization. Max allowed query time span from-to equals 31 days. API returns closed connections within timespan in UTC.",
                "tags": [
                    "Usage"
                ],
                "operationId": "getUsage.get./developer/usage",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "from",
                        "type": "string",
                        "description": "Date in format YYYY-MM-DD",
                        "required": true
                    },
                    {
                        "in": "query",
                        "name": "to",
                        "type": "string",
                        "description": "Date in format YYYY-MM-DD (this day will be included until 23:59:59.999Z)",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Usage data",
                        "schema": {
                            "$ref": "#/definitions/IUsageResponse"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden",
                        "schema": {
                            "$ref": "#/definitions/IErrorResponse"
                        }
                    }
                }
            }
        },
        "/developer/sessions/create": {
            "post": {
                "summary": "createSessionDeveloper",
                "description": "Create a new session (Developer API)",
                "tags": [
                    "Sessions"
                ],
                "operationId": "createSessionDeveloper.post./developer/sessions/create",
                "consumes": [
                    "application/json"
