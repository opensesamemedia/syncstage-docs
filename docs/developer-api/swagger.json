                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden",
                        "schema": {
                            "$ref": "#/definitions/IErrorResponse"
                        }
                    }
                }
            }
        },
        "/developer/sessions/list": {
            "get": {
                "summary": "listSessionsDeveloper",
                "description": "List sessions, with filtering on 'Session Name' or Tags (Developer API)",
                "tags": [
                    "Recordings"
                ],
                "operationId": "listSessionsDeveloper.get./developer/sessions/list",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "query",
                        "name": "projectId",
                        "type": "string",
                        "required": true
                    },
                    {
                        "in": "query",
                        "name": "nextToken",
                        "type": "string",
                        "required": false
                    },
                    {
                        "in": "query",
                        "name": "maxResults",
                        "type": "integer",
                        "required": false
                    },
                    {
                        "in": "query",
                        "name": "sessionName",
                        "type": "string",
                        "required": false
                    },
                    {
                        "in": "query",
                        "name": "tagKey",
                        "type": "string",
                        "required": false
                    },
                    {
                        "in": "query",
                        "name": "tagValue",
                        "type": "string",
                        "required": false
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of Sessions",
                        "schema": {
                            "$ref": "#/definitions/ISession"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden",
                        "schema": {
                            "$ref": "#/definitions/IErrorResponse"
                        }
                    }
                }
            }
        }
    },
